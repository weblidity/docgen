{
  "name": "prodpage-cli",
  "version": "1.0.13",
  "description": "CLI tool to generate a start-up set of files and folders for company products documentation hosted on a Docusaurus-powered site.",
  "main": "src/index.js",
  "keywords": [
    "cli",
    "docgen",
    "documentation-generation",
    "documentation-generator",
    "documentation-scaffolding",
    "documentation",
    "docusaurus-scaffold",
    "docusaurus",
    "generator",
    "markdown",
    "scaffold",
    "scaffolding"
  ],
  "bin": {
    "prodpage": "./bin/cli.js"
  },
  "scripts": {
    "test": "jest",
    "start": "node ./bin/cli.js",
    "format:js": "prettier --write \"**/*.js\"",
    "format:json": "prettier --write \"**/*.json\"",
    "format:md": "prettier --write \"**/*.md\"",
    "format:yaml": "prettier --write \"**/*.yaml\"",
    "format": "npm run format:js && npm run format:json && npm run format:md && npm run format:yaml"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "child-process": "^1.0.2",
    "commander": "^12.1.0",
    "cross-spawn": "^7.0.3",
    "file-easy": "^1.0.3",
    "glob": "^11.0.3",
    "gray-matter": "^4.1.0",
    "hbsr": "^1.0.11",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.4.1"
  },
  "devDependencies": {
    "jest": "^30.0.5",
    "prettier": "^3.6.2"
  }
}
